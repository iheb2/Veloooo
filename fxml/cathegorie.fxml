<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="425.0" prefWidth="595.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CathegorieController">
    <stylesheets>
        <URL value="@cathegorie.css" />
    </stylesheets>
   <children>
      <ImageView fx:id="image" fitHeight="439.0" fitWidth="606.0" layoutX="1.0" layoutY="2.0" pickOnBounds="true" preserveRatio="true">
            <image>
            <Image url="@../images/header-bg.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="table" layoutX="60.0" layoutY="14.0" onMouseClicked="#modifiermethode" prefHeight="164.0" prefWidth="481.0">
        <columns>
          <TableColumn fx:id="coltype" prefWidth="480.0" text="Type" />
        </columns>
      </TableView>
      <TextField fx:id="type" layoutX="94.0" layoutY="197.0" promptText="Type" />
      <Text fill="#fffefe" layoutX="46.0" layoutY="214.0" strokeType="OUTSIDE" strokeWidth="0.0" text="type" />
      <Button fx:id="ajouter" layoutX="324.0" layoutY="197.0" mnemonicParsing="false" onAction="#ajouter" prefHeight="25.0" prefWidth="95.0" text="ajouter" />
      <Button fx:id="modifier" layoutX="388.0" layoutY="257.0" mnemonicParsing="false" onAction="#modifier" prefHeight="25.0" prefWidth="95.0" text="modifier" />
      <Button fx:id="supprimer" layoutX="435.0" layoutY="197.0" mnemonicParsing="false" onAction="#supprimer" prefHeight="25.0" prefWidth="95.0" text="supprimer" />
      <ImageView fx:id="logo" fitHeight="200.0" fitWidth="176.0" layoutX="419.0" layoutY="237.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/logo.png" />
         </image>
      </ImageView>
      <Button fx:id="retour" layoutX="34.0" layoutY="368.0" mnemonicParsing="false" onAction="#retour" prefHeight="25.0" prefWidth="95.0" text="retour" />
   </children>
</AnchorPane>
